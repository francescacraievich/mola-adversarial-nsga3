[flake8]
max-line-length = 100
max-complexity = 10
exclude =
    .git,
    __pycache__,
    .pytest_cache,
    .venv,
    venv,
    env,
    build,
    dist,
    src/cyclonedds,
    src/rover_isaacsim
ignore =
    E203,
    W503,
    E501
per-file-ignores =
    __init__.py:F401,F403
    tests/*.py:E402
    src/nsga2/utils.py:C901
